{
  "expo": {
    "name": "Ride Sharing App",
    "slug": "ride-sharing-app",
    "orientation": "portrait",
    "version": "1.0.0",
    "icon": "./assets/images/icon.png",
    "scheme": "maps",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.malikc.rideSharing"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/svgs/Splashscreen/splash_screen_1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#00B773"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "02e2459c-0023-469c-af02-61feb54680a4"
      }
    }
  }
}
